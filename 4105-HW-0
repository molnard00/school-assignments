  {
   "cell_type": "code",
   "execution_count": 227,
   "metadata": {},
   "outputs": [],
   "source": [
    "X = np.hstack((X1_1, X2_1, X3_1))\n",
    "n = 3\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 228,
   "metadata": {},
   "outputs": [],
   "source": [
    "def predictions(theta, X, n):\n",
    "    h = np.ones((X.shape[0],1))\n",
    "    theta = theta.reshape(1,n+1)\n",
    "    for i in range(0,X.shape[0]):\n",
    "        h[i] = float(np.matmul(theta, X[i]))\n",
    "    h = h.reshape(X.shape[0])\n",
    "    return h"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 229,
   "metadata": {},
   "outputs": [],
   "source": [
    "def grad_desc(X, y, n, h, theta, alpha, iterations):\n",
    "    loss = np.ones(iterations)\n",
    "    for i in range(0,iterations):\n",
    "        theta[0] = theta[0] - (alpha/X.shape[0]) * sum(h - y)\n",
    "        for j in range(1,n+1):\n",
    "            theta[j] = theta[j] - (alpha/X.shape[0]) * sum((h-y) * X.transpose()[j]) \n",
    "        h = predictions(theta, X, n)\n",
    "        loss[i] = (1/X.shape[0]) * 0.5 * sum(np.square(h - y))\n",
    "    theta = theta.reshape(1,n+1)\n",
    "    return theta, loss"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 230,
   "metadata": {},
   "outputs": [],
   "source": [
    "def lin_reg(X, y, alpha, iterations):\n",
    "    n = X.shape[1]\n",
    "    Xones = np.ones((X.shape[0],1))\n",
    "    X = np.concatenate((Xones, X), axis = 1)\n",
    "    theta = np.zeros(n+1)\n",
    "    h = predictions(theta, X, n)\n",
    "    theta, loss = grad_desc(X, y, n, h, theta, alpha, iterations)\n",
    "    return theta, loss"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 231,
   "metadata": {},
   "outputs": [],
   "source": [
    "mean = np.ones(X.shape[1])\n",
    "std = np.ones(X.shape[1])\n",
    "for i in range(0, X.shape[1]):\n",
    "    mean[i] = np.mean(X.transpose()[i])\n",
    "    std[i] = np.std(X.transpose()[i])\n",
    "    for j in range(0, X.shape[0]):\n",
    "        X[j][i] = (X[j][i] - mean[i])/std[i]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 232,
   "metadata": {},
   "outputs": [],
   "source": [
    "thetaX, loss = lin_reg(X, y, alpha, iterations)"
   ]
  },
