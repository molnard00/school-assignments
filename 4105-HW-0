  {
   "cell_type": "code",
   "execution_count": 205,
   "metadata": {},
   "outputs": [],
   "source": [
    "X1_2 = np.hstack((Xones, X1_1))  # stacks the ones array and the first column of data\n",
    "thetaX1 = np.zeros(2) # sets the initial theta values for X1 as zero"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 206,
   "metadata": {},
   "outputs": [],
   "source": [
    "def comp_loss(x, y, theta,):\n",
    "    # computes the loss of the given input and output for the current theta values\n",
    "    predictions = x.dot(theta)\n",
    "    error = np.subtract(predictions, y)\n",
    "    sqrError = np.square(error)\n",
    "    J = 1/(2*m)*np.sum(sqrError)\n",
    "    \n",
    "    return J"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 207,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "The loss for X1 at these theta values = 5.483015861682611\n"
     ]
    }
   ],
   "source": [
    "lossX1 = comp_loss(X1_2, y, thetaX1) # gets the loss for X1 with theta values of zero\n",
    "print('The loss for X1 at these theta values =', lossX1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 208,
   "metadata": {},
   "outputs": [],
   "source": [
    "def gradient_descent(x, y, theta, alpha, iterations):\n",
    "    loss_history = np.zeros(iterations)\n",
    "    # computes the gradient descent and outputs the final theta values and all the loss values to be plotted\n",
    "    for i in range(iterations):\n",
    "        predictions = x.dot(theta)\n",
    "        errors = np.subtract(predictions, y)\n",
    "        sum_delta = (alpha/m)*x.transpose().dot(errors);\n",
    "        theta = theta - sum_delta\n",
    "        loss_history[i] = comp_loss(x, y, theta)\n",
    "        \n",
    "    return theta, loss_history\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 209,
   "metadata": {},
   "outputs": [],
   "source": [
    "thetaX1 = [0., 0.]\n",
    "iterations = 1500  # sets the number of iterations to 1500 and the learning rate at 0.01\n",
    "alpha = 0.01"
   ]
  },
